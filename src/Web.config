<configuration>
  <configSections>
    <section name="quickViewConfig" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <quickViewConfig>
      <!--Absolute path to GroupDocs license file-->
      <add key="licensePath" value="E:/rabota/NewTeam/lic/GroupDocs.Viewer.NET.lic" />
      <!--Directory where all original files are stored-->
      <add key="filesDirectory" value="E:/rabota/NewTeam/Documents" />
      <!--Directory all custom fonts are stored-->
      <add key="fontsDirectory" value="E:/rabota/NewTeam/Fonts/CustomFonts/" />
      <!--Run application as service (only public API)-->
      <add key="runAsService" value="false" />
      <!--Url/link to web resources (html, js, css, etc) archive-->
      <add key="resourcesUrl" value="https://github.com/bobkovalex/QuickView/archive/master.zip" />
      <!--Enable/disable resources downloading, false for download-->
      <add key="offlineMode" value="false" />
      <!--Set default document to view-->
      <add key="defaultDocument" value="" />
      <!--Enable/disable HTMl rendering mode, false for image mode-->
      <add key="htmlMode" value="true" />
      <!--Pages to be loaded on document open, 0 for all-->
      <add key="preloadPageCount" value="0" />
      <!--Enable/disable rewrite for uploaded file if file with the same name already exists-->
      <add key="rewrite" value="true" />
      <!--Enable/disable document zoom-->
      <add key="zoom" value="true" />
      <!--Enable/disable page navigation-->
      <add key="pageSelector" value="true" />
      <!--Enable/disable text search-->
      <add key="search" value="true" />
      <!--Enable/disable thumbnails-->
      <add key="thumbnails" value="true" />
      <!--Enable/disable page rotation-->
      <add key="rotate" value="true" />
      <!--Enable/disable document download-->
      <add key="download" value="true" />
      <!--Enable/disable document upload-->
      <add key="upload" value="true" />
      <!--Enable/disable document print-->
      <add key="print" value="true" />
      <!--Enable/disable documents browse-->
      <add key="browse" value="true" />   
  </quickViewConfig>
  <appSettings>
    <add key="vs:EnableBrowserLink" value="false" />
    <add key="vs:EnableBrowserLink" value="false" />
  </appSettings>
  <system.web>
